<%= form_for Player.new do |f| %>
    <%= image_tag('http://static.nfl.com/static/content/public/static/img/fantasy/transparent/200x200/'+@player_json['esbid']+'.png') %><br>
    <h3>
      Player ID: <%= f.text_field :player_id, value: @player_json['id']  %><br>
      esbid: <%= f.text_field :esbid, value: @player_json['esbid']  %><br>
      gsisPlayerId: <%= f.text_field :gsisPlayerId, value: @player_json['gsisPlayerId']  %><br>
      Name: <%= f.text_field :name, value: @player_json['name']  %><br>
      position: <%= f.text_field :position, value: @player_json['position']  %><br>
      teamAbbr: <%= f.text_field :teamAbbr, value: @player_json['teamAbbr']  %><br>
    </h3>

    <%= f.submit 'Add Player', :class => "btn btn-primary" %>
<% end %><br>